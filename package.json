{
  "name": "limechat-discordant",
  "description": "A Discord-inspired theme for LimeChat",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "predev": "npm run clean",
    "dev": "mix watch",
    "prebuild": "npm run clean",
    "build": "mix --production",
    "clean": "rimraf dist/*",
    "deploy": "npm run build && cp dist/discordant.css ../Themes && cp -r dist/discordant-assets ../Themes && cp discordant.yaml ../Themes"
  },
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "filename-regex": "^2.0.1",
    "fs-extra": "^10.0.0",
    "klaw": "^3.0.0",
    "laravel-mix": "^6.0.19",
    "laravel-mix-nunjucks": "^1.1.0",
    "most-common-words-by-language": "^3.0.3",
    "nunjucks": "^3.2.3",
    "postcss": "^8.2.15",
    "resolve-url-loader": "^3.1.3",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "through2": "^4.0.2"
  },
  "dependencies": {
    "@fontsource/nunito": "^4.3.0",
    "openmoji": "^13.0.0"
  }
}
